{
    "projectType": "application",
    "sourceRoot": "apps/pulumi-e2e/src",
    "targets": {
        "e2e": {
            "executor": "@nrwl/workspace:run-commands",
            "options": {
                "commands": [
                    "npx nx run nx-esbuild:build",
                    "npx nx run pulumi:build",
                    "npx vitest --run --config apps/pulumi-e2e/vitest.config.ts"
                ],
                "parallel": false
            }
        }
    }
}
