{
  "name": "nx-generators",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx run-many --target=build --all",
    "test": "nx run-many --target=test --all",
    "lint": "nx workspace-lint && nx run-many --all --target=lint",
    "e2e": "nx run-many --target=e2e --all",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "preinstall": "npx only-allow pnpm",
    "changesets": "changesets",
    "generate:esbuild-options:build": "pnpx typescript-json-schema node_modules/esbuild/lib/main.d.ts BuildOptions -o libs/nx-esbuild/src/executors/build/schema.json && pnpx json -I -f libs/nx-esbuild/src/executors/build/schema.json -e 'this.cli=\"nx\"' && pnpx json -I -f libs/nx-esbuild/src/executors/build/schema.json -e 'this.title=\"ESBuild Build Executor\"'",
    "generate:esbuild-options:serve": "pnpx typescript-json-schema node_modules/esbuild/lib/main.d.ts BuildOptions -o libs/nx-esbuild/src/executors/serve/schema.json && pnpx json -I -f libs/nx-esbuild/src/executors/serve/schema.json -e 'this.cli=\"nx\"' && pnpx json -I -f libs/nx-esbuild/src/executors/serve/schema.json -e 'this.title=\"ESBuild Serve Executor\"'",
    "~~~NOTE~~~": "entryPoints and watch dont work properly and need manual changes -- see 44808fb",
    "generate:esbuild-options": "pnpm run generate:esbuild-options:build && pnpm run generate:esbuild-options:serve"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/core": "^11.2.12",
    "@changesets/changelog-github": "^0.4.0",
    "@changesets/cli": "^2.16.0",
    "@nrwl/cli": "12.7.1",
    "@nrwl/devkit": "12.7.1",
    "@nrwl/eslint-plugin-nx": "12.7.1",
    "@nrwl/jest": "12.7.1",
    "@nrwl/linter": "12.7.1",
    "@nrwl/node": "12.7.1",
    "@nrwl/nx-plugin": "12.7.1",
    "@nrwl/tao": "12.7.1",
    "@nrwl/workspace": "12.7.1",
    "@types/jest": "26.0.23",
    "@types/js-yaml": "^4.0.1",
    "@types/node": "15.0.2",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "aws-sdk": "^2.963.0",
    "babel-jest": "^26.6.3",
    "dotenv": "10.0.0",
    "esbuild": "^0.11.20",
    "eslint": "7.26.0",
    "eslint-config-prettier": "8.3.0",
    "jest": "27.0.3",
    "json": "^11.0.0",
    "prettier": "2.3.2",
    "ts-jest": "27.0.3",
    "ts-node": "~9.1.1",
    "tslib": "^2.2.0",
    "typescript": "4.3.5",
    "typescript-json-schema": "^0.50.0",
    "vite": "^2.3.0"
  },
  "dependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "execa": "^5.0.0",
    "regenerator-runtime": "^0.13.7"
  }
}