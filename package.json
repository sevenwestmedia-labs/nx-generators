        "@typescript-eslint/parser": "7.12.0",
        "dotenv": "16.0.1",
        "esbuild": "^0.16.17",
        "eslint": "8.57.0",
        "eslint-config-prettier": "9.1.0",
        "jest": "^29.7.0",
        "json": "^11.0.0",
        "nx": "19.2.2",
