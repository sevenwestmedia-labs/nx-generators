{
    "$schema": "http://json-schema.org/schema",
    "name": "pulumi",
    "version": "0.0.1",
    "generators": {
        "init": {
            "factory": "./src/generators/init/generator",
            "schema": "./src/generators/init/schema.json",
            "description": "Creates a infrastructure project for another NX application project"
        },
        "init-standalone": {
            "factory": "./src/generators/init-standalone/generator",
            "schema": "./src/generators/init-standalone/schema.json",
            "description": "Creates a standalone infrastructure project"
        }
    }
}
