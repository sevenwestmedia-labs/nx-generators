{
    "$schema": "http://json-schema.org/schema",
    "cli": "nx",
    "title": "Build executor",
    "description": "",
    "type": "object",
    "properties": {
        "yes": {
            "description": "",
            "type": "boolean"
        },
        "environment": {
            "description": "",
            "type": "string",
            "aliases": [
                "env"
            ]
        },
        "stack": {
            "description": "Pulumi stack name",
            "type": "string"
        },
        "buildTargets": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "project": {
                        "type": "string"
                    },
                    "target": {
                        "type": "string"
                    },
                    "configuration": {
                        "type": "string"
                    }
                },
                "required": [
                    "project",
                    "target"
                ]
            }
        },
        "configurationStackFormat": {
            "type": "string"
        },
        "disableIntegrityChecking": {
            "type": "boolean"
        },
        "skipPreview": {
            "type": "boolean"
        },
        "nonInteractive": {
            "type": "boolean"
        },
        "secretsProvider": {
            "type": "string"
        }
    }
}
